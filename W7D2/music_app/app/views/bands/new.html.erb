<h1>Bands#new</h1>
<p>Create a band!</p>

<form action=<%= bands_url %> method="post">
    <input type="hidden" name="authenticity_token" value=<%= form_authenticity_token %>>
    <label for="band_name"></label>
    <input type="text" name="band[name]" id="band_name">

    <input type="submit" value="Create Band!">
</form>